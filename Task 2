<!DOCTYPE html>
<html>
<head>
  <title>Arrow Function Practice</title>
</head>
<body>
  <h2>Open the console to see the output</h2>

  <script>
    const greet = function(name) {
      console.log("Hello, " + name + "!");
    };

    const add = (a, b) => {
      return a + b;
    };

    const multiply = (a, b) => a * b;

    const logItems = (...items) => {
      items.forEach((item, index) => {
        console.log("Item " + (index + 1) + ":", item);
      });
    };

    const testThis = () => {
      console.log(this);
    };

    greet("Deepthi");
    console.log("Sum:", add(5, 3));
    console.log("Product:", multiply(4, 6));
    logItems("Pen", "Book", "Bottle");
    testThis();
  </script>
</body>
</html>

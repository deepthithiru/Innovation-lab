<!DOCTYPE html>
<html>
<head>
  <title>Function Examples</title>
</head>
<body>
  <h2>Open the console to see the output</h2>

  <script>
    function greet(name) {
      console.log("Hello, " + name + "!");
    }

    function add(a, b) {
      console.log("Sum:", a + b);
    }

    function multiplyAll(...numbers) {
      let result = 1;
      for (let num of numbers) {
        result *= num;
      }
      console.log("Product:", result);
    }

    function listAll() {
      for (let i = 0; i < arguments.length; i++) {
        console.log("Arg", i + 1 + ":", arguments[i]);
      }
    }

    greet("Deepthi");
    add(10, 20);
    multiplyAll(2, 3, 4);
    listAll("Apple", "Banana", "Cherry");
  </script>
</body>
</html>
